{
  "name": "compounding-helper",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:backup": "env-cmd -x pg_dump --data-only --column-inserts --schema=public --exclude-table='_prisma_migrations' \"$npm_config_db\" > .db-backup/${npm_config_name}_compounding_db_backup_$(date +\"%Y%m%dT%H%M%SZ\").sql",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "bash prisma/seed.sh"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^2.9.11",
    "@prisma/client": "^4.10.1",
    "axios": "^1.3.3",
    "babel-plugin-superjson-next": "^0.4.5",
    "date-fns": "^2.29.3",
    "lodash": "^4.17.21",
    "next": "^13.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-icons": "^4.7.1",
    "react-to-print": "^2.14.11",
    "rrule": "^2.7.2",
    "superjson": "^1.12.2",
    "swr": "^2.0.3",
    "type-fest": "^2.19.0",
    "zod": "^3.20.2",
    "zod-validation-error": "^1.3.0"
  },
  "devDependencies": {
    "@styled-jsx/plugin-sass": "^4.0.2",
    "@svgr/webpack": "^6.5.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "git-format-staged": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "prettier": "^2.8.4",
    "prisma": "^4.13.0",
    "sass": "^1.58.1",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "^4.9.5"
  }
}
