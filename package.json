{
  "name": "compounding-helper",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:backup": "env-cmd -x pg_dump --data-only --column-inserts --schema=public --exclude-table='_prisma_migrations' \"$npm_config_db\" > .db-backup/${npm_config_name}_compounding_db_backup_$(date +\"%Y%m%dT%H%M%SZ\").sql",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "bash prisma/seed.sh"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.1.0",
    "@prisma/client": "^4.14.0",
    "@tanstack/react-table": "^8.9.2",
    "axios": "^1.4.0",
    "babel-plugin-superjson-next": "^0.4.5",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "next": "^13.4.1",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-to-print": "^2.14.12",
    "rrule": "^2.7.2",
    "superjson": "^1.12.3",
    "swr": "^2.1.5",
    "type-fest": "^3.10.0",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.0"
  },
  "devDependencies": {
    "@styled-jsx/plugin-sass": "^4.0.2",
    "@svgr/webpack": "^8.0.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.1.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "git-format-staged": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "sass": "^1.62.1",
    "stylelint": "^15.6.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "5.0.2"
  }
}
