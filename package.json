{
  "name": "compounding-helper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -w",
    "test": "jest",
