{
  "name": "compounding-helper",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:backup": "env-cmd -x pg_dump --data-only --column-inserts --schema=public --exclude-table='_prisma_migrations' \"$npm_config_db\" > .db-backup/compounding_db_backup_$(date +\"%Y%m%dT%H%M%SZ\").sql",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "bash prisma/seed.sh"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.9.10",
    "@prisma/client": "^4.5.0",
    "axios": "^1.1.3",
    "babel-plugin-superjson-next": "^0.4.4",
    "lodash": "^4.17.21",
    "next": "^13.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.1",
    "react-icons": "^4.6.0",
    "react-to-print": "^2.14.7",
    "superjson": "^1.11.0",
    "swr": "^1.3.0",
    "type-fest": "^2.19.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@styled-jsx/plugin-sass": "^4.0.2",
    "@svgr/webpack": "^6.5.1",
    "@types/lodash": "^4.14.187",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "git-format-staged": "^3.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "sass": "^1.56.0",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^4.8.4"
  }
}
